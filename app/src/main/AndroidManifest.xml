<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/loginbackground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/loginbackground"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoachTicketBookingApp"
        tools:targetApi="31">
        <activity
            android:name=".ui.FeedBackListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.FeedBackActivity"
            android:exported="false" />
        <activity
            android:name=".ui.BookingUserInfoActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MyTicketDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.TicketStatisticTrip"
            android:exported="false" />
        <activity
            android:name=".ui.TicketStatisticDayMonth"
            android:exported="false" />
        <activity
            android:name=".ui.TicketStatisticUser"
            android:exported="false" />
        <activity
            android:name=".ui.ManageBookingTicket"
            android:exported="false" />
        <activity
            android:name=".ui.ManageTicket"
            android:exported="false" />
        <activity
            android:name=".ui.ManageTrip"
            android:exported="false" />
        <activity
            android:name=".ui.ManageCoach"
            android:exported="false" />
        <activity
            android:name=".ui.ManageCategory"
            android:exported="false" />
        <activity
            android:name=".ui.ManageUser"
            android:exported="false" />
        <activity
            android:name=".ui.Manage"
            android:exported="false" />
        <activity
            android:name=".ui.LoginAdmin"
            android:exported="false" />
        <activity
            android:name=".ui.PaymentResultActivity"
            android:exported="false" />
        <activity
            android:name=".ui.PaymentMethodActivity"
            android:exported="true"
            tools:ignore="AppLinkUrlError">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="app"
                    android:scheme="demozpdk" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.TripListFoundDetailsActivity"
            android:exported="true" />
        <activity
            android:name=".ui.TripListFoundActivity"
            android:exported="true" />
        <activity
            android:name=".ui.SearchResultsActivity"
            android:exported="true" />
        <activity
            android:name=".ui.Welcomepage"
            android:exported="false" />
        <activity
            android:name=".ui.RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".ui.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MainUIActivity"
            android:exported="true"
            android:theme="@style/Theme.CoachTicketBookingApp" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>